{
  "name": "Room Booking System",
  "description": "Booking system for faith rooms.",
  "repository": "https://github.com/teamCAAP/Multi-Faith-Room-Booking-System",
  "logo": "https://pbs.twimg.com/profile_images/961758302261665798/BEDU-uIs_400x400.jpg",
  "keywords": ["booking", "room", "faith", "laravel"],
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "9.5"
      }
    }
  ],
  "scripts": {
    "postdeploy": "php artisan migrate"
  },
  "env": {
    "APP_KEY": {
      "description": "A secret key for verifying the integrity of signed cookies.",
      "generator": "secret"
    },
    "DB_CONNECTION": {
      "description": "PGSQL",
      "value": ""
    },
    "DB_DATABASE": {
      "description": "PGSQL",
      "value": ""
    },
    "DB_HOST": {
      "description": "PGSQL",
      "value": ""
    },
    "DB_USERNAME": {
      "description": "PGSQL",
      "value": ""
    },
    "DB_PASSWORD": {
      "description": "PGSQL",
      "value": ""
    },
    "DB_PORT": {
      "description": "PGSQL",
      "value": "5432"
    }
  }
}
